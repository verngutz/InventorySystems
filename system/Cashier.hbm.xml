<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="system" >
<class name="Cashier" table="CASHIER">

<composite-id name="index" class="CashierPK">
    <key-property name="index" type="integer" column="INDEX"/>
    <key-many-to-one name="store" cascade="all" lazy="false" class="Store" column="STORE_ID"/>
</composite-id>
	 
<property name="cash" type="double" column="CASH"/>

<property name="online" type="boolean" column="ONLINE_STATUS"/>
</class>

</hibernate-mapping>
	